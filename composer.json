{
    "name": "vimeo/vimeo-api",
    "description": "Official PHP library for the Vimeo API.",
    "homepage": "https://github.com/vimeo/vimeo.php",
    "keywords": ["vimeo", "video"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Vimeo",
            "homepage": "http://vimeo.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ankitpokhrel/tus-php",
                "version": "v2.3.0",
                "source": {
                    "url": "https://github.com/nijilawrence/tus-php",
                    "type": "git",
                    "reference": "drupal-php-8"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1.0 || ^8.0",
        "ext-curl": "*",
        "ext-json":"*",
        "ankitpokhrel/tus-php": "v2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Vimeo\\": "src/Vimeo"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vimeo\\": "tests/Vimeo"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^1.0",
        "phpunit/phpunit": "^7.4",
        "vimeo/psalm": "^2.0"
    },
    "scripts": {
        "coverage": "phpunit --coverage-html reports/",
        "tests": [
            "psalm",
            "phpunit"
        ]
    }
}
